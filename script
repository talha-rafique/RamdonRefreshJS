// ==UserScript==
// @name        Auto Reload Protopage
// @namespace   http://blog.monstuff.com/archives/cat_greasemonkey.html
// @description Reload pages every 5 minutes
// @include     https://www.fiverr.com/talharafique462?up_rollout=true
// @grant       none
// ==/UserScript==

// based on code by Julien Couvreur
// and included here with his gracious permission

var numMinutes = Math.floor(Math.random() * 5);

setTimeout (function(){location.reload();},numMinutes *60*1000);
console.log(numMinutes);
